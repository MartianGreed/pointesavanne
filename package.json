{
  "name": "pointesavanne",
  "description": "Villa Pointe Savanne website",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --watch index.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:coverage:lcov": "bun test --coverage --coverage-reporter=lcov --coverage-dir=./coverage",
    "test:features": "node --import tsx node_modules/@cucumber/cucumber/bin/cucumber.js",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@cucumber/cucumber": "11",
    "@types/bun": "latest",
    "tsx": "^4.21.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
